{
  "version": 2,
  "name": "counter",
  "scope": "brunobernardino",
  "alias": ["beta.counter.brn.sh"],
  "github": {
    "enabled": false
  },
  "env": {
    "NODE_ENV": "production",
    "NODE_PATH": "."
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NODE_PATH": "."
    }
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/static/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    }
  ]
}
